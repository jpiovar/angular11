{
  "name": "angular11",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock": "json-server --port 3000 --watch ./src/assets/mock/server.js --routes ./src/assets/mock/routes.json",
    "postinstall": "ngcc",
    "stylelint": "stylelint **/*.scss",
    "project:init": "npm cache clean --force && npm cache verify && npm i -g @angular/cli && npm i -g @angular/compiler && npm i -g @angular/compiler-cli && npm i -g typescript && npm install",
    "lint:staged": "run-with-changes i -r tslint-files \"*.ts\" -r stylelint-files \"*.{scss,css}\"",
    "lint:changed": "run-with-changes iwu -r tslint-files \"*.ts\" -r stylelint-files \"*.{scss,css}\"",
    "tslint-files": "cd ./../../ && tslint",
    "stylelint-files": "cd ./../../ && stylelint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/material-moment-adapter": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/router-store": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngrx/store-devtools": "^11.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.6.0",
    "classlist.js": "^1.1.20150312",
    "dompurify": "^2.2.9",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "ngx-device-detector": "^2.1.1",
    "ngx-toastr": "^14.0.0",
    "postcss": "^8.3.5",
    "rxjs": "~6.6.0",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.19.0",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.14",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.15",
    "codelyzer": "^6.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "run-with-changes": "^1.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.6",
    "json-server": "^0.16.3"
  }
}
